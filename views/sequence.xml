<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="secuencia_citas" model="ir.sequence">
            <field name="name">Secuencias de citas</field>
            <field name="code">clinica.citas</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="prefix">CITA</field>
            <field name="padding">6</field>
        </record>

        <record id="secuencia_hospital" model="ir.sequence">
            <field name="name">Secuencias de hospitalizaciones</field>
            <field name="code">clinica.hospitalizacion</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="prefix">HOSP</field>
            <field name="padding">4</field>
        </record>
    </data>

</odoo>